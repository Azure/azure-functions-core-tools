<Project>

    <Target Name="RemovePythonWorkerOnUnsupportedRids" BeforeTargets="AssignTargetPaths">
        <ItemGroup
            Condition="'$(RuntimeIdentifier)' == 'win-arm64' Or '$(RuntimeIdentifier)' == 'linux-arm64'">
            <PackageReference Remove="Microsoft.Azure.Functions.PythonWorker" />
        </ItemGroup>
    </Target>

</Project>